{
  "entryFile": "src/app.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["src/**/*Controller.ts"],
  "spec": {
    "basePath": "/api/1",
    "outputDirectory": "build",
    "specFileBaseName": "openapi",
    "specVersion": 3,
    "name": "elaws-openapi-proxy",
    "description": "An OpenAPI proxy for e-LAWS API (the official API for Japanese laws data)."
  },
  "routes": {
    "routesDir": "src",
    "basePath": "/api/1"
  }
}
